{
	"manifest_version": 2,
	"name": "ABTasty debug",
	"description": "Debugger ab-tests",
	"version": "1.0",
	"permissions": ["http://*/*", "https://*/*", "activeTab", "storage"],
	"icons": {
		"128": "images/icon-128x128.png"
	},
	"content_scripts": [
		{
			"matches": ["http://*/*", "https://*/*"],
			"js": ["static/content-script.js"]
		}
	],
	"background": {
		"scripts": ["static/background.js"]
	},
	"web_accessible_resources": ["static/page-script.js"],
	"browser_action": {
		"default_popup": "popup.html"
	}
}
